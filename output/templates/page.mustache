    <title>{{title}} - 宁静海手游官网</title>
    <meta name="keywords" content="海量,福利">
    <meta name="description" content="{{pre}}">
    <link href="{{resLocation}}/statics/css/style.css" rel="stylesheet" type="text/css"/>
    <link href="{{resLocation}}/statics/css/custom.css" rel="stylesheet" type="text/css"/>
    <script type="text/javascript" src="{{resLocation}}/statics/js/jquery.min.js"></script>
    <script type="text/javascript" src="{{resLocation}}/statics/js/jquery.sgallery.js"></script>
    <script type="text/javascript" src="{{resLocation}}/statics/js/scroll.js"></script>
